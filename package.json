{
    "name": "ramped_frontend",
    "version": "0.0.1",
    "description": "Admin portal",
    "scripts": {
        "dev": "next dev -p 3000",
        "pm2:start": "pm2 start ecosystem.config.js",
        "pm2:start:dev": "pm2 start ecosystem.dev.config.js",
        "pm2:stop": "pm2 stop ecosystem.config.js",
        "pm2:stop:dev": "pm2 stop ecosystem.dev.config.js",
        "pm2:delete": "pm2 delete ecosystem.config.js",
        "pm2:delete:dev": "pm2 delete ecosystem.dev.config.js",
        "start": "npm run build && npm run pm2:start",
        "start:dev": "npm run build && npm run pm2:start:dev",
        "stop": "npm run pm2:stop",
        "build": "next build",
        "test": "jest --forceExit --coverage --verbose --runInBand --detectOpenHandles --updateSnapshot",
        "format": "eslint --fix src/**/*.ts src/**/*.tsx",
        "prettier:lint": "prettier --write src/**/*.ts src/**/*.tsx",
        "next:lint": "next lint"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com"
    },
    "keywords": [
        "Ramped",
        "Admin",
        "Typescript",
        "PM2",
        "Next.js",
        "Redux",
        "Tailwindcss",
        "Fontawesome"
    ],
    "author": "Ramped",
    "license": "ISC",
    "dependencies": {
        "@fortawesome/free-solid-svg-icons": "^6.5.2",
        "@fortawesome/react-fontawesome": "^0.2.2",
        "@headlessui/react": "^2.0.4",
        "@hookform/resolvers": "^3.4.2",
        "@next/bundle-analyzer": "^14.2.3",
        "@redux-devtools/extension": "^3.3.0",
        "@typescript-eslint/eslint-plugin": "^7.11.0",
        "autoprefixer": "^10.4.19",
        "axios": "^1.7.2",
        "bcryptjs": "^2.4.3",
        "classnames": "^2.5.1",
        "eslint-webpack-plugin": "^4.2.0",
        "fork-ts-checker-webpack-plugin": "^9.0.2",
        "framer-motion": "^11.2.9",
        "localforage": "^1.10.0",
        "lodash": "^4.17.21",
        "moment-locales-webpack-plugin": "^1.2.0",
        "next": "^14.2.3",
        "next-pwa": "^5.6.0",
        "next-seo": "^6.5.0",
        "path-type": "^5.0.0",
        "rc-pagination": "^4.1.0",
        "rc-table": "^7.46.0",
        "react": "^18.3.1",
        "react-hook-form": "^7.51.5",
        "react-redux": "^9.1.2",
        "react-toastify": "^10.0.5",
        "redux": "^5.0.1",
        "redux-logger": "^3.0.6",
        "redux-persist": "^6.0.0",
        "redux-promise-middleware": "^6.2.0",
        "redux-thunker": "^0.2.5",
        "schema-utils": "^4.2.0",
        "tailwindcss": "^3.4.3",
        "tailwindcss-rtl": "^0.9.0",
        "yup": "^1.4.0"
    },
    "resolutions": {
        "@types/react": "^18.3.1"
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "^14.2.3",
        "@types/bcryptjs": "^2.4.6",
        "@types/lodash": "^4.17.4",
        "@types/react": "^18.3.3",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "^3.2.5"
    }
}
